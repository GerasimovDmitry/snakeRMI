<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane style="-fx-background-color: #cbc2ff;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Main">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <bottom>
        <Button fx:id="okButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" mnemonicParsing="false" prefHeight="26.0" prefWidth="182.0" style="-fx-background-color: #ac9af5;" text="Ок" textAlignment="CENTER" BorderPane.alignment="BOTTOM_RIGHT">
            <BorderPane.margin>
                <Insets left="10.0" top="10.0" />
            </BorderPane.margin>
        </Button>
    </bottom>
    <center>
        <TextField fx:id="nameField" BorderPane.alignment="CENTER" />
    </center>
    <top>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <Label alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="22.0" prefWidth="177.0" text="GAME OVER" textAlignment="CENTER" textFill="#e440cc">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Elephant" size="17.0" />
                    </font>
                </Label>
                <Label prefHeight="18.0" prefWidth="155.0" text="Введите имя" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets bottom="6.0" right="10.0" top="2.0" />
                    </VBox.margin>
               <font>
                  <Font name="Elephant" size="14.0" />
               </font>
                </Label>
            </children>
        </VBox>
    </top>
</BorderPane>
